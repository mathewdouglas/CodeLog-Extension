{
	"name": "codelog",
	"displayName": "CodeLog",
	"description": "Access CodeLog Snippets",
	"version": "0.0.1",
	"publisher": "MathewDouglas",
	"icon": "Icon_4_IOS.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/mathewdouglas/CodeLog"
	},
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:codelog.getSnippet",
		"onCommand:codelog.refresh"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "codelog.getSnippet",
				"title": "Get Snippet"
			},
			{
				"command": "codelog.refresh",
				"title": "CodeLog: Refresh Snippets"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"axios": "^0.21.1"
	}
}
